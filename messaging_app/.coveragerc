[run]
source = .
omit = 
    */venv/*
    */migrations/*
    */tests/*
    */test_*
    manage.py
    */settings/*
    */__pycache__/*
    .venv/*
    */admin.py
    */apps.py
    */wsgi.py
    */asgi.py

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    raise AssertionError
    raise NotImplementedError
    if __name__ == .__main__.:
    class .*\bProtocol\):
    @(abc\.)?abstractmethod
show_missing = True
skip_covered = False
precision = 2

[html]
directory = htmlcov
title = Messaging App Coverage Report

[xml]
output = coverage.xml
